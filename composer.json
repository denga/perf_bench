{
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.13.1",
        "symfony/cache": "^6.1.3"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2.6",
        "doctrine/data-fixtures": "^1.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
